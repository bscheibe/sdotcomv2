<meta http-equiv="Content-Type" content="text/html" charset="UTF-8">
<meta name="viewport" content="width=device-width">
<meta name="viewport" content="initial-scale=1.0">

<!-- Basic Meta Data -->
<title><%= title %></title> 
<meta name="description" content="<%= description %>" />
<meta name="author" content="<%= author %>">

<!-- Schema.org markup for Google+ --> 
<meta itemprop="name" content="<%= title %>"> 
<meta itemprop="description" content="<%= description %>"> 
<meta itemprop="image" content="/images/macbook-header-live.png">

<!-- Open Graph data --> 
<meta property="og:title" content="<%= title %>" /> 
<meta property="og:type" content="website" /> 
<meta property="og:url" content="http://brentscheibelhut.com/" />
<meta property="og:image" content="/images/macbook-header.png" />
<meta content='500' property='og:image:width' />
<meta content='500' property='og:image:height' />
<meta property="og:description" content="<%= description %>" /> 
<meta property="og:site_name" content="Brent Scheibelhut" />
<meta content='<%= description %>' name='description' />
<meta content='Brent, Scheibelhut, Design, Freelance, Hack, Code, Artist, Writer, Athlete' name='keywords' />
<!--<meta property="fb:admins" content="Facebook numberic ID" />--> <!-- If we get a facebook page place ID here -->

<!-- Other META -->
<link href='/images/macbook-header.png' rel='image_src' />
<meta content='en_US' property='og:locale' />
<meta content='brent.scheibelhut' property='fb:admins' />
